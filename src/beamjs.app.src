{application, beamjs,
 [
  {description, "beam.js"},
  {vsn, "0.0.1 (%sha%)"},
  {registered, []},
  {applications, [
				  erlv8,
                  kernel,
                  stdlib
                 ]},
  {modules, [beamjs, beamjs_app, beamjs_sup, beamjs_repl_sup, beamjs_repl, beamjs_repl_console, beamjs_js_formatter,
			 beamjs_mod_exports, beamjs_mod_require, beamjs_mod_sys, beamjs_mod_console, beamjs_mod_repl, beamjs_mod_os]},
  {mod, { beamjs_app, []}},
  {env, [
		 {module_bundles,
		  [
		   {default, 
			[{default,
			  [
			   {require, beamjs_mod_require},
			   {console, beamjs_mod_console},
			   {'__bundles__', beamjs_mod_bundles}
			  ]},
			 {available,
			  [
			   {"repl", beamjs_mod_repl}
			  ]
			 }]},
		   {node_compat,
			[
			 {available,
			  [{"events", beamjs_mod_events},
			   {"sys", beamjs_mod_sys},
			   {"util", beamjs_mod_util},
			   {"fs", beamjs_mod_fs}
			  ]}
			 ]},
		   {erlang,
			[
			 {available,
			  [{"dist", beamjs_mod_dist},
			   {"messaging", beamjs_mod_messaging}
			  ]}
			 ]},
		   {stdlib,
			[
			 {available,
			  [{"os", beamjs_mod_os}
			  ]}
			 ]},
		   {commonjs,
			[
			 {available,
			  [{"test", beamjs_mod_test}
			  ]}
			 ]}
		   
		   ]}
		]}
 ]}.
